section.changelog {
    max-width: 800px;
    margin: 0 3em 3em 3em;
}
.changelog h1 {
    margin-top: 3em;
    font-weight: 600;
}
.changelog h1:first-child {
    margin-top: 0.5em;
}
.changelog h2 {
    font-weight: 600;
    margin-top: 1em;
    margin-bottom: .5em;
}
.changelog ul {
    padding-left: 2em;
}
.changelog li {
    margin-top: .5em;
}
