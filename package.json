{
  "name": "server",
  "version": "1.5.1",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production DB=production ./bin/www",
    "dev": "./bin/start-mongodb-redis-nginx.sh && NODE_ENV=development nodemon ./bin/www",
    "icon": "./node_modules/svg-sprite/bin/svg-sprite.js --symbol --symbol-dest='' --symbol-sprite='dist/icon/sprite.svg' dist/icon/*.svg",
    "export-db": "mongodump --db zunka -u admin --authenticationDatabase admin --gzip --archive=./dump/$(date +\"%Y-%h-%d@%T\").gz",
    "export-db-old": "mongodump --db zunka -u admin --authenticationDatabase admin --out ./dump/$(date +\"%Y-%h-%d@%T\")",
    "test": "./bin/start-mongodb-redis-nginx.sh && NODE_ENV=test mocha -R spec ./test/spec.js -t 2000",
    "coverage": "./bin/start-mongodb-redis-nginx.sh && NODE_ENV=test nyc mocha -R spec ./test/spec.js -t 6000"
  },
